{
  "name": "javascript-status",
  "version": "1.0.1",
  "main": "build/index.js",
  "description": "Javascript Status is tiny HTTP status code utility library for readability.",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test-cov": "babel-node isparta cover --report html --report text node_modules/.bin/_mocha",
    "build": "babel src --out-dir build",
    "clean": "rm -rf build",
    "lint": "eslint src test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaibhavmule/javscript-status.git"
  },
  "keywords": [
    "http",
    "status",
    "status-code",
    "http-status-code",
    "javscript-status"
  ],
  "author": "Vaibhav Mule <vaibhavmule135@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vaibhavmule/javscript-status/issues"
  },
  "homepage": "https://github.com/vaibhavmule/javscript-status#readme",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "mocha": "^2.4.5"
  }
